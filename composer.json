{
    "name": "fc9/blueprint",
    "description": "API Blueprint documentation generator.",
    "keywords": [
        "fc9",
        "api",
        "laravel",
        "blueprint",
        "docs"
    ],
    "homepage": "https://github.com/fc9/laravel-blueprint",
    "license": "MIT",
    "type": "library",
    "authors": [{
        "name": "Fabio Cabral",
        "email": "fabiocabralsantos@gmail.com"
    }],
    "require": {
        "php": "~7.2.5",
        "illuminate/support": "~7.0",
        "illuminate/filesystem": "~7.1",
        "doctrine/annotations": "~1.8.0",
        "phpdocumentor/reflection-docblock": "~5.1.0"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "~3.5.4",
        "phpunit/phpunit": "~8.0"
    },
    "autoload": {
        "psr-4": {
            "Fc9\\Blueprint\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Fc9\\Blueprint\\Tests\\": "tests"
        }
    },
     "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
